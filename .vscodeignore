.vscode/**
.vscode-test/**
.git/**
.github/**

# Source code (keep only compiled output)
src/**
shared/**

# Node modules (both root and webview)
node_modules/**
webview-ui/node_modules/**

# Webview source and config (keep only dist)
webview-ui/src/**
webview-ui/public/**
webview-ui/.vscode/**
webview-ui/package.json
webview-ui/package-lock.json
webview-ui/tsconfig*
webview-ui/svelte.config.js
webview-ui/vite.config.ts
webview-ui/.gitignore
webview-ui/README.md
webview-ui/index.html

# Build artifacts and configs (keep only /out)
scripts/**
**/*.tsbuildinfo
tsconfig.*
!tsconfig.json # Keep base tsconfig if needed by runtime? Usually no.

# Other dev files
.gitignore
.DS_Store
*.vsix
eslint.config.mjs # Exclude ESLint config

# Exclude tests
src/test/**
.vscode-test.mjs

# Keep specific necessary files (ensure these are correct)
!out/** # Keep the compiled output
!webview-ui/dist/** # Keep the webview build
!package.json # Keep the manifest
!README.md
!CHANGELOG.md
!LICENSE # Keep license file
!images/** # Keep images