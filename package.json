{
  "name": "gitorial",
  "displayName": "Gitorial",
  "description": "Interactive VS Code extension that guides you step-by-step through Git-based tutorials with inline explanations and easy navigation.",
  "version": "0.0.1",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Education",
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitorial.cloneTutorial",
        "title": "Gitorial: Clone New Tutorial"
      },
      {
        "command": "gitorial.openTutorial",
        "title": "Gitorial: Open Tutorial"
      },
      {
        "command": "gitorial.showHomePage",
        "title": "Gitorial: Show Home Page"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "markdown-it": "^14.1.0",
    "simple-git": "^3.27.0"
  }
}
